<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.QuestionController">
    <children>
        <TextField fx:id="questionField" layoutX="127.0" layoutY="64.0" promptText="Question"/>
        <TextField fx:id="answerField" layoutX="127.0" layoutY="139.0" promptText="answer"/>
        <Button fx:id="okButton" layoutX="101.0" layoutY="246.0" mnemonicParsing="false" onAction="#okBut" text="ok"/>
        <Button fx:id="backButton" layoutX="237.0" layoutY="246.0" mnemonicParsing="false" onAction="#back"
                text="back"/>
    </children>
</AnchorPane>
