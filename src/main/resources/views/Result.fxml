<?xml version="1.0" encoding="UTF-8"?>


<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.*?>

<AnchorPane stylesheets="/css/loginStyle.css" prefHeight="620.0" prefWidth="760.0"
            xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.ResultController">
    <TableView fx:id="resultTable" layoutX="80.0" layoutY="22.0" opacity="0.96" prefHeight="400.0" prefWidth="600.0"
    >
        <columns>
            <TableColumn fx:id="playerColumn" editable="false" prefWidth="200.0" sortType="DESCENDING" sortable="false"
                         text="Player"/>
            <TableColumn fx:id="pointColumn" editable="false" prefWidth="213.0" sortType="DESCENDING" sortable="false"
                         text="Point"/>
            <TableColumn fx:id="timeColumn" editable="false" prefWidth="186.0" sortType="DESCENDING" sortable="false"
                         text="Time"/>
        </columns>
        <cursor>
            <Cursor fx:constant="DEFAULT"/>
        </cursor>
    </TableView>
    <JFXButton fx:id="restartButton" layoutX="80.0" layoutY="492.0" onAction="#onClickedRestartButton" prefHeight="40.0"
               prefWidth="101.0" text="Restart"/>
    <JFXButton fx:id="backButton" layoutX="330.0" layoutY="492.0" onAction="#onClickedBackButton" prefHeight="40.0"
               prefWidth="101.0" text="Back"/>
    <JFXButton fx:id="quitButton" layoutX="579.0" layoutY="492.0" onAction="#onClickedQuitButton" prefHeight="40.0"
               prefWidth="101.0" text="Quit"/>
</AnchorPane>
